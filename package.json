{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "4.0.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.0",
    "bizcharts": "^3.5.8",
    "connected-react-router": "^6.8.0",
    "customize-cra": "^0.9.1",
    "draft-js": "^0.11.4",
    "file-saver": "^2.0.2",
    "husky": "^4.2.3",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.8",
    "markdown-it": "^10.0.0",
    "medium-draft": "^0.5.18",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-markdown-editor-lite": "^1.0.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.4.2",
    "xlsx": "^0.15.6"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "git": "git add . && git commit -m",
    "postgit": "git pull && git push && git status",
    "update": "pm2 deploy production update",
    "formate": "./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  }
}
